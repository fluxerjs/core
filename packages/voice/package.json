{
  "name": "@fluxerjs/voice",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.4",
  "description": "Voice support for Fluxer bots",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fluxerjs/collection": "workspace:*",
    "@fluxerjs/core": "workspace:*",
    "@fluxerjs/types": "workspace:*",
    "@livekit/rtc-node": "^0.13.24",
    "opus-decoder": "^0.7.0",
    "prism-media": "^1.3.5",
    "tweetnacl": "^1.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}